{
  "expo": {
    "name": "NotiaNote",
    "slug": "notianote",
    "scheme": "notianote",
    "version": "4.0.22",
    "orientation": "portrait",
    "icon": "./assets/notianote-violet-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/notianote-violet-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2E1065"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "e17b020c-b1bf-4f61-9fa6-7bf681626451"
      }
    },
    "owner": "finocchiarodev",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "extraProguardRules": "-keep class com.google.android.gms.internal.consent_sdk.** { *; }"
          }
        }
      ],
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "NotiaNote a besoin de la caméra pour scanner votre carte de cantine."
        }
      ],
      [
        "react-native-permissions",
        {
          "iosPermissions": [
            "AppTrackingTransparency",
            "Camera"
          ]
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-5759305174337781~8980538178",
          "iosAppId": "ca-app-pub-5759305174337781~8976608670",
          "userTrackingUsageDescription": "Autorisez-vous les publicités personnalisées sur NotiaNote ?"
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/notianote-violet-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dontbyshai.notianote",
      "versionCode": 57,
      "userInterfaceStyle": "automatic"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dontbyshai.notianote",
      "buildNumber": "4",
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSCameraUsageDescription": "NotiaNote a besoin de la caméra pour scanner votre carte de cantine.",
        "GADApplicationIdentifier": "ca-app-pub-5759305174337781~8976608670",
        "NSUserTrackingUsageDescription": "Autorisez-vous les publicités personnalisées sur NotiaNote ?"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-5759305174337781~8980538178",
    "ios_app_id": "ca-app-pub-5759305174337781~8976608670"
  }
}